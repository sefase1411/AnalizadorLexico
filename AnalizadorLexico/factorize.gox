func mod(x int, y int) int {
    return x - x / y * y;
}

func is_prime(n int) bool {
    if n < 2 {
        return false;
    }
    var i int = 2;
    while i*i <= n {
        if n % i == 0 {
            return false;
        }
        i = i + 1;
    }
    return true;
}

